{
    "login": {
        "method":"post",
        "template": "/api/accounts/login",
        "exceptions":{
            "LoginError" :401
        }
    },
    "getOrders": {
        "method": "get",
        "template": "/api/accounts/{userId}/orders/{orderId}",
        "incomingHeaderMapping":{
            "Authorization": "token"
        }
        ,
        "exceptions":{
            "SessionExpired" :401
        }

    },
    "addItem": {
        "method": "put",
        "template": "/api/accounts/orders/{orderId}?porco1={porco}&maiale2={maiale}",
        "incomingHeaderMapping":{
            "Authorization": "token"
        },       
        "exceptions":{
            "SessionExpired" :401
        }
    }
}